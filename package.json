{
  "name": "nakiriapi",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register index.ts",
    "build": "next build",
    "start": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register index.ts",
    "lint": "eslint . --ext .ts --ext .tsx",
    "typeorm": "node ./node_modules/typeorm/cli.js --config ormconfig.cli.js",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:rollback": "npm run typeorm -- migration:revert",
    "migration:generate": "npm run typeorm -- migration:generate -n",
    "migrate": "npm run migration:run",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint -- --fix"
    ]
  },
  "dependencies": {
    "@sentry/nextjs": "^6.11.0",
    "axios": "^0.21.1",
    "bull": "^3.29.0",
    "camelcase": "^6.2.0",
    "clsx": "^1.1.1",
    "cookie-parser": "^1.4.5",
    "date-fns": "^2.22.1",
    "express": "^4.17.1",
    "formik": "^2.2.9",
    "is-url": "^1.2.4",
    "lodash.pull": "^4.1.0",
    "lodash.remove": "^4.7.0",
    "lodash.truncate": "^4.4.2",
    "mysql2": "^2.2.5",
    "next": "^11.0.1",
    "next-api-paginate": "^1.0.0",
    "next-auth": "^3.27.3",
    "next-bar": "^1.0.1",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "redis": "^3.1.2",
    "sass": "^1.29.0",
    "sweetalert2": "^11.0.18",
    "sweetalert2-react-content": "^4.1.0",
    "typeorm": "^0.2.34",
    "url-parse": "^1.5.1",
    "uuid": "^8.3.2",
    "ws": "^8.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@types/bull": "^3.15.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/inquirer": "^7.3.3",
    "@types/is-url": "^1.2.30",
    "@types/lodash.pull": "^4.1.6",
    "@types/lodash.remove": "^4.7.6",
    "@types/lodash.truncate": "^4.4.6",
    "@types/node": "^16.3.1",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.14",
    "@types/redis": "^2.8.32",
    "@types/url-parse": "^1.4.3",
    "@types/uuid": "^8.3.1",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "autoprefixer": "^10.3.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "inquirer": "^8.1.1",
    "postcss": "^8.3.5",
    "prettier": "^2.4.1",
